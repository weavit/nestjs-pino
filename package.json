{
  "name": "nestjs-pino",
  "version": "1.3.0",
  "description": "Pino logger for NestJS",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "rm -rf ./dist && tsc --p tsconfig.build.json",
    "report": "cat ./coverage/lcov.info | coveralls",
    "example": "rm -rf ./dist && tsc && LOG_LEVEL=debug node dist/example/main"
  },
  "engineStrict": false,
  "engines": {
    "node": "^4.7 || >=6.9 || >=7.3 || >=8.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamolegga/nestjs-pino.git"
  },
  "keywords": [
    "pino",
    "nestjs",
    "nest.js",
    "nest",
    "logger"
  ],
  "author": "Oleg Repin <iamolegga@gmail.com> (http://github.com/iamolegga)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamolegga/nestjs-pino/issues"
  },
  "homepage": "https://github.com/iamolegga/nestjs-pino#readme",
  "dependencies": {
    "@sentry/node": "5.29.0",
    "@types/pino-http": "5.0.6",
    "@types/pino-multi-stream": "5.1.0",
    "express-ctx": "0.1.1",
    "pino": "6.7.0",
    "pino-http": "5.3.0",
    "pino-multi-stream": "5.1.1"
  },
  "devDependencies": {
    "@nestjs/common": "7.6.1",
    "@nestjs/core": "7.6.1",
    "@nestjs/platform-express": "7.6.1",
    "@nestjs/platform-fastify": "7.6.1",
    "@nestjs/testing": "7.6.1",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.19",
    "@types/memorystream": "0.3.0",
    "@types/node": "14.14.12",
    "@types/supertest": "2.0.10",
    "coveralls": "3.1.0",
    "express": "4.17.1",
    "jest": "26.6.3",
    "memorystream": "0.3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "tslint": "6.1.2",
    "typescript": "4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node"
  }
}
